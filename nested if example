#include<stdio.h>
#include<ctype.h>
int main(){
    char ch;
    printf("Enter Alphabet ");
    scanf("%c", &ch);
    if(isalpha(ch)) {
        ch= toupper (ch);
        if (ch=='A'|| ch=='E'|| ch=='I'|| ch=='O'|| ch=='U')
        printf("It is vowel!!!");
    else
        printf("It is consonant!!!");
    return 0;
}
    }
